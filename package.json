{
  "name": "react-webapp",
  "version": "0.0.1",
  "description": "isomorphic WebApp use react, redux, react-router, webpack",
  "main": "index.js",
  "scripts": {
    "test": "npm run clean && npm run build && cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "start": "cross-env NODE_ENV=development node ./src/server",
    "start:prod": "npm run clean && npm run build && cross-env NODE_ENV=production node ./src/server",
    "build": "webpack -p --config ./webpack.client.prod.js && webpack -p --config ./webpack.server.prod.js",
    "clean": "rm -rf build"
  },
